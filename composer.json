{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": { 
            "Zerbikat\\BackendBundle\\": "src/Zerbikat/BackendBundle" ,
            "App\\": "src",
            "ApiBundle\\": "src/ApiBundle",
            "FrontendBundle\\": "src/FrontendBundle",
            "UserBundle\\": "src/UserBundle"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Tests\\Zerbikat\\BackendBundle\\": "tests/Zerbikat/BackendBundle",
            "App\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "bmatzner/fontawesome-bundle": "^4.6",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-cache-bundle": "^1.2",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/ckeditor-bundle": "^2.0",
        "friendsofsymfony/jsrouting-bundle": "^2.8",
        "friendsofsymfony/rest-bundle": "2.7",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "guzzlehttp/guzzle": "6.3",
        "guzzlehttp/psr7": "1.4.0",
        "javiereguiluz/easyadmin-bundle": "^1.13",
        "jms/serializer-bundle": "^2.1",
        "knplabs/knp-menu-bundle": "^3.1",
        "nelmio/api-doc-bundle": "2.13.5",
        "sensio/framework-extra-bundle": "^5.0",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/flex": "^1.21",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/ldap": "4.4.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/polyfill-php72": "^1.10",
        "symfony/swiftmailer-bundle": "^3.1",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/webpack-encore-bundle": "^1.17",
        "symfony/yaml": "4.4.*",
        "white-october/pagerfanta-bundle": "^1.3",
        "whiteoctober/tcpdf-bundle": "1.0"
    },
    "require-dev": {
        "easycorp/easy-log-handler": "^1.0",
        "symfony/debug-bundle": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/maker-bundle": "*",
        "symfony/phpunit-bridge": "4.4.*",
        "symfony/stopwatch": "4.4.*",
        "symfony/web-profiler-bundle": "4.4.*"
    },
    "config": {
        "platform": {
            "php": "7.2.34"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install --symlink --relative %PUBLIC_DIR%": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install --clear=drop": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.*"
        }
    }
}
