{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": { 
            "Zerbikat\\BackendBundle\\": "src/Zerbikat/BackendBundle" ,
            "App\\": "src",
            "ApiBundle\\": "src/ApiBundle",
            "FrontendBundle\\": "src/FrontendBundle",
            "UserBundle\\": "src/UserBundle"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Tests\\Zerbikat\\BackendBundle\\": "tests/Zerbikat/BackendBundle",
            "App\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.2",
        "amorebietakoudala/user-bundle": "^1.4.6",
        "babdev/pagerfanta-bundle": "*",
        "doctrine/doctrine-bundle": "^2.0",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.5",
        "friendsofsymfony/ckeditor-bundle": "^2.0",
        "friendsofsymfony/jsrouting-bundle": "^2.8",
        "friendsofsymfony/rest-bundle": "^3.6",
        "guzzlehttp/guzzle": "6.3",
        "guzzlehttp/psr7": "1.4.0",
        "jms/serializer-bundle": "^5.0",
        "knplabs/knp-menu-bundle": "^3.1",
        "nelmio/api-doc-bundle": "*",
        "qipsius/tcpdf-bundle": "^2.0",
        "sensio/framework-extra-bundle": "^6.2",
        "symfony/apache-pack": "^1.0",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/flex": "^1.21",
        "symfony/form": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/ldap": "5.4.*",
        "symfony/monolog-bundle": "^3.6",
        "symfony/polyfill-php72": "^1.10",
        "symfony/translation": "5.4.*",
        "symfony/twig-bundle": "5.4.*",
        "symfony/validator": "5.4.*",
        "symfony/webpack-encore-bundle": "^1.17",
        "symfony/yaml": "5.4.*"
    },
    "require-dev": {
        "easycorp/easy-log-handler": "^1.0",
        "rector/rector": "^1.2",
        "symfony/debug-bundle": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/maker-bundle": "*",
        "symfony/phpunit-bridge": "5.4.*",
        "symfony/stopwatch": "5.4.*",
        "symfony/web-profiler-bundle": "5.4.*"
    },
    "config": {
        "platform": {
            "php": "7.2.34"
        },
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "symfony/flex": true
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install --tag=4.21.0 --clear=drop": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        }
    }
}
