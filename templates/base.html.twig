<!DOCTYPE html>
<html base="{{ app.request.baseUrl }}" lang="{{ app.request.locale|split('_')[0] }}">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->

    <title>{% block title %}ZERBItzuen KATalogoa{% endblock %}</title>
    {% block stylesheets %}
        {{ encore_entry_link_tags('app') }}
    {% endblock %}
    <link rel="shortcut icon" type="image/x-icon" href="{{ asset('build/images/favicon.ico') }}">
    <link rel="icon" sizes="192x192" href="{{ asset('build/images/touch-icon-192x192.png') }}">
    <link rel="icon" sizes="128x128" href="{{ asset('build/images/touch-icon-128x128.png') }}">
    <link rel="apple-touch-icon" size="128x128" href="{{ asset('build/images/touch-icon-128x128.png') }}" />
    <link rel="apple-touch-icon-precomposed" size="128x128" href="{{ asset('build/images/touch-icon-128x128.png') }}" />

    <meta name="application-name" content="Zerbikat" />
    <meta name="msapplication-TileColor" content=" #FFFFFF" />
    <meta name="msapplication-square70x70logo" content="{{ asset('build/images/touch-icon-70x70.png') }}" />
    <meta name="msapplication-square150x150logo" content="{{ asset('build/images/touch-icon-150x150.png') }}" />

    {{ include('google_analytics.html.twig') }}
</head>
<body role="document">

{% block menua %}
    <div class="affix-top" data-spy="affix" data-offset-top="5">
        {#{% if is_granted('ROLE_KUDEAKETA') %}#}
        {{ include('_menu_nagusia.html.twig') }}
        {#{% endif %}#}
        
        {% block bigarren_menua %}
            <nav id="secondary_menu" class="navbar navbar-default navbar-lower">
                <div class="container">
                    <div class="navbar-header">
                        {% block menuTitle %}{% endblock menuTitle %}
                    </div>
                    <div id="navbar" class="navbar-collapse collapse navbar-right">
                        {% block menuOptions %}{% endblock menuOptions %}
                    </div>
                </div>
            </nav>
        {% endblock bigarren_menua %}
    </div>
{% endblock menua %}

{% block body %}{% endblock %}

{% block footer %}{% endblock %}

{% block javascripts %}
    {{ encore_entry_script_tags('app') }}
{% endblock %}
</body>
</html>